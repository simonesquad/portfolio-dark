ntact.js",["92"],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/Dashboard.js",["93"],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/Navigation.js",["94"],"import React from 'react';\nimport ButtonLinks from './common/Button';\n\nimport { \n    Flex,\n    Box,\n    Button,\n    ButtonGroup,\n    IconButton,\n    useColorMode,\n    Spacer\n} from '@chakra-ui/react';\nimport { FaSun, FaMoon } from 'react-icons/fa';\nimport { createBreakpoints } from '@chakra-ui/theme-tools';\n\nconst breakpoints = createBreakpoints({\n    sm: '30em',\n    md: '48em',\n    lg: '62em',\n    xl: '80em',\n    '2xl': '96em',\n  })\n\nconst Navigation = ({ oneBtnTxt, oneBtnLink, twoBtnTxt, twoBtnLink, threeBtnTxt, threeBtnLink, fourBtnTxt, fourBtnLink, fiveBtnTxt, fiveBtnLink }) => {\n    const { colorMode, toggleColorMode } = useColorMode();\n    return (\n        <Flex \n            h='10vh'\n            w={{\n                sm: '75%',\n                md: '100%',\n                lg: '100%'\n            }}\n            >\n        <ButtonGroup \n            spacing={{\n                sm: '1',\n                lg:'8'\n            }}\n            variant='ghost'\n            marginTop='2vh'\n            >\n            <Button>\n                <ButtonLinks \n                    text={oneBtnTxt} \n                    link={oneBtnLink} \n                />\n            </Button>\n            <Button>\n                <ButtonLinks \n                    text={twoBtnTxt} \n                    link={twoBtnLink} \n                />\n            </Button>\n            <a rel=\"noreferrer\" href=\"https://www.github.com/simonesquad\" target=\"_blank\">\n            <Button>\n                Github\n            </Button>\n            </a>\n            <Button>\n                <ButtonLinks \n                    text={threeBtnTxt} \n                    link={threeBtnLink} \n                />\n            </Button>\n            <Button>\n                <ButtonLinks \n                    text={fourBtnTxt} \n                    link={fourBtnLink} \n                />\n            </Button>\n            <Button>\n                <ButtonLinks \n                    text={fiveBtnTxt} \n                    link={fiveBtnLink} \n                />\n            </Button>\n        </ButtonGroup>\n        <Spacer />\n        <Box p='3'>\n        <IconButton \n            icon={ colorMode === 'light' ?  <FaMoon /> : <FaSun color='white' />}\n            isRound='true'\n            size='lg'\n            alignSelf='flex-end'\n            onClick={toggleColorMode}\n        />\n        </Box>\n        </Flex>\n    )\n}\n\nexport default Navigation","/Users/simoneballard/Desktop/portfolio-dark/simone/src/ContactForm.js",["95"],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/common/Button.js",[],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/common/Greeting.js",["96"],"import React from 'react'\n\nimport {\n    Box,\n    Heading,\n    Text\n} from '@chakra-ui/react';\nimport { createBreakpoints } from '@chakra-ui/theme-tools';\n\nconst breakpoints = createBreakpoints({\n    sm: '30em',\n    md: '48em',\n    lg: '62em',\n    xl: '80em',\n    '2xl': '96em',\n  })\n\nconst Greeting = () => {\n  return (\n    <Box w='100vw' h='70vh'>\n        <Box \n          w={{\n            sm: '79vw',\n            lg: '50vw' \n          }}\n          h='50vh'\n          marginTop='15%'\n          marginLeft='10%'\n        >\n          <Box h={{\n            sm: '12vh',\n            lg: '9vh'\n          }}>\n            <Heading>Thank You for Your Message</Heading>\n          </Box>\n            <Text>I am wandering the world somewhere lost in code and words. But also I will get back to you shortly!</Text>\n        </Box>\n    </Box>\n  )\n}\n\nexport default Greeting","/Users/simoneballard/Desktop/portfolio-dark/simone/src/layout/index.js",[],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/common/Sidebar.js",[],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/common/Header.js",[],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/common/NLink.js",["97"],"import React from 'react'\nimport { NavLink } from 'react-router-dom';\n\nimport {\n    Flex,\n    Text\n} from \"@chakra-ui/react\";\n\nexport default function NLink({ link, ...rest }) {\n    const { label, href } = link;\n\n    return (\n        <div>\n        <NavLink to={href} passHref>\n            <a>\n                <Flex\n                    align=\"center\"\n                    p=\"4\"\n                    mx=\"4\"\n                    borderRadius=\"lg\"\n                    role=\"group\"\n                    cursor=\"pointer\"\n                    _hover={{\n                        bg: \"gray.200\",\n                        color: \"black\",\n                        fontWeight: \"bold\",\n                        textDecoration: \"none\"\n                    }}\n                    {...rest}\n                >\n                    <Text fontSize=\"1.2rem\">{label}</Text>\n                </Flex>\n            </a>\n        </NavLink>\n        </div>\n    );\n}","/Users/simoneballard/Desktop/portfolio-dark/simone/src/MobileMenu.js",[],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/Vertical.js",["98","99","100"],"/Users/simoneballard/Desktop/portfolio-dark/simone/src/Horizontal.js",["101"],"import React from 'react';\nimport Face from './assets/face.jpeg';\n\nimport {\n    Box,\n    Heading,\n    Image,\n    Text,\n    Spacer,\n    HStack,\n} from '@chakra-ui/react';\nimport { createBreakpoints } from '@chakra-ui/theme-tools';\n\nconst breakpoints = createBreakpoints({\n    sm: '30em',\n    md: '48em',\n    lg: '62em',\n    xl: '80em',\n    '2xl': '96em',\n  })\n\nfunction Horizontal() {\n\n    return(\n\n        <HStack\n                w='90%'\n                h={{\n                    md: '140vh',\n                    lg: '100vh'\n                }}\n            >\n            <Box\n                w='30vw'\n                marginLeft={{\n                    md: '5vw',\n                    lg: '7vw'\n                }}\n            >\n            <Image \n                src={Face} \n                alt='mural'\n                h={{\n                    md: '50vh',\n                    lg: '55vh'\n                }}\n                w={{\n                    md: '25vw',\n                    lg: '20vw'\n                }}\n                borderRadius='50%'\n                />\n            </Box>\n\n            <Spacer />\n\n            <Box\n                w={{\n                    md: '55vw',\n                    lg: '65vw'\n                }}\n            >\n                <Box\n                    h={{\n                    md: '17vh',\n                    lg: '13vh'\n                }}\n                >\n                <Heading>Language is our key to all the worlds...</Heading>\n                \n                </Box>\n\n                <Box\n                    h={{\n                    sm: '115vh',\n                    md: '75vh',\n                    lg: '57vh'\n                    }}\n                >\n                <Text>I realized early on in my studies and travels that how you communicate an idea is <Text as='b'>half the value of the idea itself.</Text> From working in environmental science during the 2010's, I learned that effecitively communicating the facts of climate change was no small task. <Text as='b'>Facts can easily get diluted or misconstrued in the general narrative behind alterior motives.</Text> None of us realized what a force social media would become for both the good and detriment of society. Coming of age in this era has shaped my approach towards every aspect of my work. Now I can see the good in rapidly communicating and connecting across the globe - especially being remote during a pandemic and needing to still gain information from the international coder community. <Text as='b'>Moving into the next decade I have thoroughly honed my writing style and feel confident in my expert level editing and curation abilities.</Text> While you can still find me on social media and writing articles here and there, I have shifted my focus to my personal projects towards narrative non-fiction stories. I hope you'll join me somewhere on the journey and if you'd like to work together on a project - feel free to reach out.</Text>\n                </Box>\n\n                <Box\n                    h={{\n                        sm: '17vh',\n                        md: '12vh',\n                        lg:'10vh'\n                    }}\n                >\n                <Heading as='h2' size='lg' >Your story is your legacy,\n                </Heading> \n                <Heading as='h2' size='lg' >make it a good one.</Heading>\n                </Box>\n            </Box>\n            </HStack>\n    )\n}\n\nexport default Horizontal",{"ruleId":"102","severity":1,"message":"103","line":14,"column":7,"nodeType":"104","messageId":"105","endLine":14,"endColumn":12},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"102","severity":1,"message":"110","line":15,"column":7,"nodeType":"104","messageId":"105","endLine":15,"endColumn":18},{"ruleId":"102","severity":1,"message":"111","line":21,"column":10,"nodeType":"104","messageId":"105","endLine":21,"endColumn":18},{"ruleId":"102","severity":1,"message":"112","line":22,"column":22,"nodeType":"104","messageId":"105","endLine":22,"endColumn":35},{"ruleId":"102","severity":1,"message":"110","line":15,"column":7,"nodeType":"104","messageId":"105","endLine":15,"endColumn":18},{"ruleId":"102","severity":1,"message":"110","line":10,"column":7,"nodeType":"104","messageId":"105","endLine":10,"endColumn":18},{"ruleId":"102","severity":1,"message":"110","line":22,"column":7,"nodeType":"104","messageId":"105","endLine":22,"endColumn":18},{"ruleId":"102","severity":1,"message":"110","line":16,"column":7,"nodeType":"104","messageId":"105","endLine":16,"endColumn":18},{"ruleI